#include <stdio.h>

int main(void) {
double tab[10];
double tab2[10];
double tab3[10]; 
double start;
double stop;
double r;
double skalar=0;
  
  //printf("Podaj 1. liczbe: ");
  scanf("%lf", &start);  
  
  //printf("Podaj ostatnia liczbe: ");
  scanf("%lf", &stop);

  r=(stop-start)/9;
  int i;
  for(i=0; i<10; i++){
    tab[i]=stop-(i*r);
  }
   for(i=0; i<10; i++){
   tab2[i]=tab[9-i];
  }
  for(i=0; i<10; i++){
    tab3[i]=tab[i]*tab2[i];
  }
 for(i=0; i<10; i++){
   skalar=(skalar+tab3[i]);
 }
  printf ("%.4lf ", skalar);

    return 0;
}
